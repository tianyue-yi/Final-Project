let stu = [];

function setup() {
  createCanvas(400, 400);
  for (let i = 0; i < 7; i ++) {
    stu.push.(new students ());
  }
}

function draw() {
  background(220);
}

class students {
  constructor (x, y) {
    this.x = x;
    this.y = y;
  }
  
  vibe () { // loadAnimation
  
  }
  
  display () { // draw one stu and his chair
    ellipse (this.x, this.y, 50, 50);
    rect (this.x, this.y, 30, 45);
      
    
  }
  
  chat () {
    
  }
}
